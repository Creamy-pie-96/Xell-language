{
  "Function Definition": {
    "prefix": "fn",
    "body": [
      "fn ${1:name}(${2:params}):",
      "    ${3:pass.}",
      ";"
    ],
    "description": "Define a new function"
  },
  "Function with Return": {
    "prefix": "fnr",
    "body": [
      "fn ${1:name}(${2:params}):",
      "    give ${3:expression}.",
      ";"
    ],
    "description": "Define a function with a return value"
  },
  "Forward Declaration": {
    "prefix": "fwd",
    "body": "fn ${1:name}(${2:params}).",
    "description": "Forward-declare a function"
  },
  "If Statement": {
    "prefix": "if",
    "body": [
      "if ${1:condition}:",
      "    ${2:body.}",
      ";"
    ],
    "description": "If conditional block"
  },
  "If-Else": {
    "prefix": "ife",
    "body": [
      "if ${1:condition}:",
      "    ${2:then_body.}",
      "else:",
      "    ${3:else_body.}",
      ";"
    ],
    "description": "If-else conditional block"
  },
  "If-Elif-Else": {
    "prefix": "ifee",
    "body": [
      "if ${1:condition}:",
      "    ${2:body.}",
      "elif ${3:condition2}:",
      "    ${4:body2.}",
      "else:",
      "    ${5:else_body.}",
      ";"
    ],
    "description": "If-elif-else block"
  },
  "For Loop (range)": {
    "prefix": "forr",
    "body": [
      "for ${1:i} in range(${2:n}):",
      "    ${3:body.}",
      ";"
    ],
    "description": "For loop with range(n)"
  },
  "For Loop (range from-to)": {
    "prefix": "forft",
    "body": [
      "for ${1:i} in range(from ${2:start} to ${3:end}):",
      "    ${4:body.}",
      ";"
    ],
    "description": "For loop with range(from start to end)"
  },
  "For Loop (range from-to-step)": {
    "prefix": "forfts",
    "body": [
      "for ${1:i} in range(from ${2:start} to ${3:end} step ${4:step}):",
      "    ${5:body.}",
      ";"
    ],
    "description": "For loop with range(from start to end step s)"
  },
  "For-In Loop": {
    "prefix": "forin",
    "body": [
      "for ${1:item} in ${2:collection}:",
      "    ${3:body.}",
      ";"
    ],
    "description": "For-in loop over collection"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while ${1:condition}:",
      "    ${2:body.}",
      ";"
    ],
    "description": "While loop"
  },
  "Variable Declaration": {
    "prefix": "var",
    "body": "var ${1:name} = ${2:value}.",
    "description": "Declare a variable"
  },
  "Print": {
    "prefix": "pr",
    "body": "print(${1:expression})",
    "description": "Print a value"
  },
  "Give (Return)": {
    "prefix": "give",
    "body": "give ${1:expression}.",
    "description": "Return a value from a function"
  },
  "With (Context Manager)": {
    "prefix": "with",
    "body": [
      "with open(\"${1:filename}\", \"${2:r}\") as ${3:f}:",
      "    ${4:body.}",
      ";"
    ],
    "description": "Context manager with file open"
  },
  "Let-Be (English assignment)": {
    "prefix": "let",
    "body": "let ${1:name} be ${2:value}.",
    "description": "English-style variable assignment"
  },
  "New-Are (English list)": {
    "prefix": "new",
    "body": "new ${1:name} are [${2:items}].",
    "description": "English-style list creation"
  },
  "Method of (English method)": {
    "prefix": "of",
    "body": "${1:method}(${2:args}) of ${3:object}",
    "description": "English-style method call using 'of'"
  }
}
